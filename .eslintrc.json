{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "no-console": 0,
        "no-unused-vars": 0,
        "no-restricted-globals": 0,
        "no-useless-constructor": 0,
        "no-undef": 0,
        "import/no-extraneous-dependencies": 0,
        "consistent-return": 0,
        "no-prototype-builtins": 0,
        "import/prefer-default-export": 0,
        "no-plusplus": 0,
        "no-await-in-loop": 0        
    }
}
